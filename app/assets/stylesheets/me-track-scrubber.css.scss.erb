.track_scrubber {
  max-width: 870px;
  background: #696667;
  color: white;
  padding: 0pt 10pt 10pt 10pt;
  border: 1pt solid #353536;
  border-bottom-right-radius: 6pt;
  border-bottom-left-radius: 6pt;

  table {
    width: 100%;
  }

  th, td {
    padding: 4pt;
    text-align: right;
  }

  td {
    padding-right: 5pt;
  }
  
}
